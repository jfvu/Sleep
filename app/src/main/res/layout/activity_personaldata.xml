<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F7F7F7">
<ImageView
    android:layout_width="match_parent"
    android:layout_height="20px"
    android:background="#44366e"/>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="44px"
        android:background="#44366e">
        <ImageView
            android:id="@+id/img_back_personaldata"
            android:layout_width="11.1px"
            android:layout_height="match_parent"
            android:src="@mipmap/back3x"
            android:layout_marginLeft="12px"
            android:layout_centerVertical="true"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="22px"
            android:text="个人资料"
            android:textColor="#ffffff"
            android:gravity="center"
            android:layout_centerInParent="true"/>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="80px"
        android:background="#FEFEFE"
        android:layout_marginTop="8px"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="头像"
            android:gravity="center"
            android:textColor="#323232"
            android:textSize="18px"
            android:layout_centerVertical="true"
            android:layout_marginLeft="12px"/>
        <ImageView
            android:id="@+id/img_headnext_personaldata"
            android:layout_width="8px"
            android:layout_height="match_parent"
            android:src="@mipmap/next3x"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="12px"/>
        <ImageView
            android:layout_width="58px"
            android:layout_height="58px"
            android:src="@mipmap/littlehead3x"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="29px"/>
    </RelativeLayout>
    <ImageView
    android:layout_width="match_parent"
    android:layout_height="1px"
    android:background="#F7F7F7"/>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="48px"
        android:background="#FEFEFE">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="姓名"
            android:gravity="center"
            android:textColor="#323232"
            android:textSize="18px"
            android:layout_centerVertical="true"
            android:layout_marginLeft="12px"/>
        <ImageView
            android:id="@+id/img_namenext_personaldata"
            android:layout_width="8px"
            android:layout_height="match_parent"
            android:src="@mipmap/next3x"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="12px"/>
        <TextView
            android:id="@+id/tv_name_personaldata"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="小仙女"
            android:textSize="16px"
            android:textColor="#6b6b6b"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:gravity="center"
            android:layout_marginRight="25px"/>
    </RelativeLayout>
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="#F7F7F7"/>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="48px"
        android:background="#FEFEFE">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="性别"
            android:gravity="center"
            android:textColor="#323232"
            android:textSize="18px"
            android:layout_centerVertical="true"
            android:layout_marginLeft="12px"/>
        <ImageView
            android:id="@+id/img_sexnext_personaldata"
            android:layout_width="8px"
            android:layout_height="match_parent"
            android:src="@mipmap/next3x"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="12px"/>
        <TextView
            android:id="@+id/tv_sex_personaldata"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="女"
            android:textSize="16px"
            android:textColor="#6b6b6b"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:gravity="center"
            android:layout_marginRight="25px"/>
    </RelativeLayout>
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="#F7F7F7"/>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="48px"
        android:background="#FEFEFE">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="生日"
            android:gravity="center"
            android:textColor="#323232"
            android:textSize="18px"
            android:layout_centerVertical="true"
            android:layout_marginLeft="12px"/>
        <ImageView
            android:id="@+id/img_birthdaynext_personaldata"
            android:layout_width="8px"
            android:layout_height="match_parent"
            android:src="@mipmap/next3x"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="12px"/>
        <TextView
            android:id="@+id/tv_birthday_personaldata"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="1970-01-01"
            android:textSize="16px"
            android:textColor="#6b6b6b"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:gravity="center"
            android:layout_marginRight="25px"/>
    </RelativeLayout>
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="#F7F7F7"/>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="48px"
        android:background="#FEFEFE">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="学历"
            android:gravity="center"
            android:textColor="#323232"
            android:textSize="18px"
            android:layout_centerVertical="true"
            android:layout_marginLeft="12px"/>
        <ImageView
            android:id="@+id/img_educationnext_personaldata"
            android:layout_width="8px"
            android:layout_height="match_parent"
            android:src="@mipmap/next3x"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="12px"/>
        <TextView
            android:id="@+id/tv_education_personaldata"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="本科"
            android:textSize="16px"
            android:textColor="#6b6b6b"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:gravity="center"
            android:layout_marginRight="25px"/>
    </RelativeLayout>
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="#F7F7F7"/>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="48px"
        android:background="#FEFEFE">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="地区"
            android:gravity="center"
            android:textColor="#323232"
            android:textSize="18px"
            android:layout_centerVertical="true"
            android:layout_marginLeft="12px"/>
        <ImageView
            android:id="@+id/img_areanext_personaldata"
            android:layout_width="8px"
            android:layout_height="match_parent"
            android:src="@mipmap/next3x"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="12px"/>
        <TextView
            android:id="@+id/tv_area_personaldata"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="地区"
            android:textSize="16px"
            android:textColor="#6b6b6b"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:gravity="center"
            android:layout_marginRight="25px"/>
    </RelativeLayout>
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="#F7F7F7"/>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="48px"
        android:background="#FEFEFE">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="行业"
            android:gravity="center"
            android:textColor="#323232"
            android:textSize="18px"
            android:layout_centerVertical="true"
            android:layout_marginLeft="12px"/>
        <ImageView
            android:id="@+id/img_businessnext_personaldata"
            android:layout_width="8px"
            android:layout_height="match_parent"
            android:src="@mipmap/next3x"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="12px"/>
        <TextView
            android:id="@+id/tv_business_personaldata"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="IT"
            android:textSize="16px"
            android:textColor="#6b6b6b"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:gravity="center"
            android:layout_marginRight="25px"/>
    </RelativeLayout>
</LinearLayout>